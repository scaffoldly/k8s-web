{
  "name": "k8s-web-angular",
  "version": "1.34.0-angular",
  "description": "TypeScript Kubernetes API client for Angular generated from OpenAPI v3 specifications",
  "keywords": [
    "kubernetes",
    "k8s",
    "angular",
    "api-client",
    "openapi",
    "typescript"
  ],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/scaffoldly/k8s-web.git",
    "directory": "angular"
  },
  "bugs": {
    "url": "https://github.com/scaffoldly/k8s-web/issues"
  },
  "homepage": "https://github.com/scaffoldly/k8s-web#readme",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "build": "echo 'No build needed - publishing TypeScript source'",
    "dev": "echo 'No dev build - TypeScript source is used directly'",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "echo 'Publishing TypeScript source files'"
  },
  "peerDependencies": {
    "@angular/common": "^17.0.0",
    "@angular/core": "^17.0.0",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@angular/common": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@swc/core": "^1.15.8",
    "k8s-web-util": "*",
    "rxjs": "^7.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "zone.js": "^0.14.0"
  }
}
